{
  "name": "@passerelle/enclosure-nuxt",
  "version": "0.0.1",
  "description": "passerelle enclosure module for nuxt3",
  "repository": {
    "type": "git",
    "url": "git@github.com:hacomono-lib/passerelle.git",
    "directory": "packages/enclosure-nuxt"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxt-module-build --stub && nuxt prepare playground",
    "preview": "nuxt dev playground --port 5175",
    "playground:build": "nuxt build playground",
    "playground:start": "nuxt start playground --port 5175",
    "build": "nuxt-module-build",
    "test": "run-p test:*",
    "test:spec": "vitest run",
    "test:type": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.2",
    "@nuxt/test-utils": "^3.5.1",
    "@types/node": "^18",
    "nuxt": "^3.5.0",
    "vitest": "^0.32.2"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "nuxt": {
      "optional": true
    }
  }
}
